            <string>// attach pause to the player object.
if (global.pause) {
    exit;
}


///Player's input
key_right = keyboard_check(vk_right);
key_left = -keyboard_check(vk_left);
key_jump = keyboard_check_pressed(vk_space);

//react to inputs
move = key_left + key_right;
hspd = move * movespeed;
if (vspd &lt; 10){
    vspd += grav;
}

if (place_meeting(x,y + 1, obj_room)){
    vspd = key_jump * -jumpspeed;
}
 // horizontal collision
if (place_meeting(x + hspd, y, obj_room)){
    while (!place_meeting(x + sign(hspd), y, obj_room)){
        x += sign(hspd);
    }
    hspd = 0;
}

// vertical collision
if (place_meeting(x, y + vspd, obj_room)){
    while (!place_meeting(x, y + sign(vspd), obj_room)){
        y += sign(vspd);
    }
    vspd = 0;
}

if ( y &gt; 416 ){
    room_goto(rm_gameover);
}
x += hspd;
y += vspd;

</string>
