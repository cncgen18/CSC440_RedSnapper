        menuOp = 0;
    }
}

var push;
push = max(keyboard_check_released(vk_enter), keyboard_check_released(vk_space), 0);
if (push == 1){
    switch(menuOp){
    case 0:
        room_goto_next();
        break;
    case 1:
        game_end();
        break;
    }
}
</string>
